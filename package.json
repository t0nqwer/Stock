{
  "name": "stock",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "Server": "node server/server.js",
    "watch": "webpack --config webpack.common.js --watch",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild --force",
    "build": "webpack --config webpack.prod.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "make:linux": "electron-builder --linux",
    "make:macos": "electron-builder --mac",
    "make:win": "electron-builder --win --x64"
  },
  "author": "",
  "license": "ISC",
  "build": {
    "productName": "KhwantaPos",
    "appId": "com.jerga.chatapp",
    "files": [
      "build/**/*",
      "assets/**/*",
      "utils/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/**/*",
      "index.html",
      "main.js",
      "preload.js",
      "./server/server.js"
    ],
    "extraResources": [
      "server/**/*",
      "./assets/**",
      "./prisma/**",
      "node_modules/@prisma/**/*",
      "node_modules/.prisma/client/**"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/images/react_app_logo.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 810,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "icon": "assets/icon/android-chrome-512x512.png",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "autoprefixer": "^10.4.13",
    "electron": "^22.1.0",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "postcss": "^8.4.21",
    "prisma": "^4.9.0",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@prisma/client": "^4.9.0",
    "@syncfusion/ej2": "^20.4.44",
    "@syncfusion/ej2-react-popups": "^20.4.44",
    "babel-loader": "^9.1.2",
    "better-sqlite3": "^8.0.1",
    "css-loader": "^6.7.3",
    "knex": "^2.4.2",
    "postcss-loader": "^7.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.8.0",
    "react-toastify": "^9.1.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
